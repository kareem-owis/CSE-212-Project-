void Total_distance(long double long2,long double lat2 ){

  double dlong = long2 * ( M_PI / 180.0 ) - long1 * ( M_PI / 180.0 );
  double dlat = lat2 * ( M_PI / 180.0 ) - lat1 * ( M_PI / 180.0 );
  double change = pow(sin(dlat / 2), 2) +
                          cos(lat1) * cos(lat2) *
                          pow(sin(dlong / 2), 2);


}
